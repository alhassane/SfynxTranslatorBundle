{
  "name": "sfynx-project/translator-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle provides entity and models of classes allowing to work with symfony translation words.",
  "keywords": ["translation", "internationalization"],
  "homepage": "http://pigroupe.github.io/cmf-sfynx/",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Etienne de Longeaux",
      "email": "etienne.delongeaux@gmail.com"
    },
    {
      "name": "Sfynx Community",
      "homepage": "https://github.com/pigroupe/SfynxTranslatorBundle/contributors"
    }
  ],

  "require": {
    "symfony/framework-bundle": "~2.3|~3.0"
  },

  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpunit/phpunit": "*"
  },

  "suggest": {
  },

  "conflict": {
  },

  "autoload": {
    "psr-0": { "Sfynx\\TranslatorBundle": "" }
  },

  "target-dir": "Sfynx/TranslatorBundle",

  "extra": {
    "branch-alias": {
      "dev-master": "2.3-dev"
    }
  }
}
